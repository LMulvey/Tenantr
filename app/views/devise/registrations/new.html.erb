<div class="row">

  <div class="col-md-6 col-md-offset-3">
    
    <div class="well">
      <h2 class="text-center">Register for <strong>Tenantr</strong>
        <% if params[:plan] == '2' %>
        <span class="label label-success">PRO</span></h2>
      <h3 class="text-center"><strong>PRO</strong> Account</h3>
      <p class="text-center">Register for the Pro Account for $10/month to gain full access to the <strong>Tenantr</strong> application.</p>
    <% else %>
    <span class="label label-primary">Basic</span></h2>
      <h3 class="text-center">Basic Account</h3>
      <p class="text-center">Rgister for free and get basic access to the <strong>Tenantr</strong> application.</p> 
    <% end %>
    <br />
      <% if params[:plan] == '2' %>
        <%= render "pro_form" %>
      <% else %>
        <%= render "basic_form" %>
      <% end %>
      <%= render "devise/shared/links" %>
      
    </div>
  </div>
</div>